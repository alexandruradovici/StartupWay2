{
    "README": [
      "THIS FILE SHOULD NOT BE USED FOR COMPILATION.",
      "THIS FILE IS WRITTEN FOR TSSERVER TO UNDERSTAND OUR MONOREPO.",
      "SEE `compile.tsconfig.json` FOR COMPILATION CONFIGURATION."
    ],
    "extends": "./configs/base.tsconfig.json",
    "include": [
        "packages/*/src",
        "distributions/*/src", "packages/users/image.d.ts", "packages/teams/image.d.ts",
    ],
    "compilerOptions": {
      "allowJs": true,
      "noEmit": true,
      "baseUrl": ".",
      "module": "es2015",
	  "types": ["vuetify"],
      "paths": {
        "@startupway/database/*": [
            "packages/database/*"
        ],
        "@startupway/main/*": [
            "packages/main/*"
        ],
        "@startupway/public/*": [
            "packages/public/*"
        ],
        "@startupway/workspace/*": [
            "packages/workspace/*"
        ],
        "@startupway/menu/*": [
            "packages/menu/*"
        ],
        "@startupway/users/*": [
            "packages/users/*"
        ],
        "@startupway/bmodelcanvas/*": [
            "packages/bmodelcanvas/*"
        ],
        "@startupway/feed/*": [
            "packages/feed/*"
        ],
        "@startupway/workshop/*": [
            "packages/workshop/*"
        ],
        "@startupway/uploaddownload/*": [
            "packages/uploaddownload/*"
        ],
        "@startupway/teams/*": [
            "packages/teams/*"
        ],
        "@startupway/admin/*": [
            "packages/admin/*"
        ],
        "@startupway/daemons/*": [
            "packages/daemons/*"
        ]
      }
    }
  }